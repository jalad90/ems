<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.ems.dto.SendMailDto" table="feedback_detail">
		<id name="feedbackId" type="integer">
			<column name="feedback_id"></column>
			<generator class="native"></generator>
		</id>

		<property name="from" column="from_nn" type="string"></property>
		<property name="to" column="to_nn" type="string"></property>
		<property name="subject" column="subject_nn" type="string"></property>
		<property name="text" column="text_nn" type="string"></property>
		<property name="userName" column="username_nn" type="string"></property>
		<property name="passWord" column="password_nn" type="string"></property>
	</class>

</hibernate-mapping>